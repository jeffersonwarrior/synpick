{
  "name": "synclaude",
  "version": "1.6.0",
  "description": "Interactive model selection tool for Claude Code with Synthetic AI models",
  "main": "dist/index.js",
  "bin": {
    "synclaude": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js",
    "build:tsc": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "npm run build && node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "node scripts/ensure-bashrc.js",
    "postlink": "node scripts/ensure-bashrc.js",
    "prepublishOnly": "npm test",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "claude",
    "anthropic",
    "synthetic",
    "ai",
    "cli",
    "model-selection",
    "typescript",
    "nodejs"
  ],
  "author": "Synclaude Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffersonwarrior/synclaude"
  },
  "bugs": {
    "url": "https://github.com/jeffersonwarrior/synclaude/issues"
  },
  "homepage": "https://github.com/jeffersonwarrior/synclaude#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "ink": "^3.2.0",
    "react": "^18.3.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.27",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "react-devtools-core": "^4.28.5",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "scripts/"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
