{
  "name": "synpick",
  "version": "1.6.2",
  "description": "Interactive model selection tool for Claude Code with Synthetic AI models",
  "main": "dist/index.js",
  "bin": {
    "synpick": "./dist/cli/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js && node postbuild.mjs",
    "build:tsc": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "npm run build && node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "node scripts/ensure-bashrc.cjs",
    "postlink": "node scripts/ensure-bashrc.cjs",
    "prepublishOnly": "npm test",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "claude",
    "anthropic",
    "synthetic",
    "ai",
    "cli",
    "model-selection",
    "typescript",
    "nodejs"
  ],
  "author": "Synpick Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffersonwarrior/synpick"
  },
  "bugs": {
    "url": "https://github.com/jeffersonwarrior/synpick/issues"
  },
  "homepage": "https://github.com/jeffersonwarrior/synpick#readme",
  "dependencies": {
    "axios": "^1.8.2",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "ink": "^6.6.0",
    "react": "^19.0.0",
    "zod": "^4.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@jest/globals": "^29.7.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.8",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "react-devtools-core": "^6.1.5",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.51.0"
  },
  "files": [
    "dist/",
    "version.txt",
    "README.md",
    "USAGE.md",
    "LICENSE",
    "scripts/"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
